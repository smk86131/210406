    "import random\n",
    "correct_answer = random.randint(1, 20)\n",
    "\n",
    "while True :\n",
    "    number = int(input(\"숫자를 입력하세요: \"))\n",
    "\n",
    "    if correct_answer == number :\n",
    "        break\n",
    "    elif correct_answer > number :\n",
    "        print(number, \"보다 큽니다\")\n",
    "    else :\n",
    "        print(number, \"보다 작습니다\")\n",
    "\n",
    "print(\"정답입니다\")\n"
